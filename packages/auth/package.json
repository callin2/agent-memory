{
  "name": "@agent-memory/auth",
  "version": "1.0.0",
  "description": "Shared authentication library for Agent Memory System - JWT generation/validation, API key operations, and Express middleware",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "npm run build",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "authentication",
    "jwt",
    "api-key",
    "express-middleware",
    "auth"
  ],
  "author": "Agent Memory System",
  "license": "MIT",
  "peerDependencies": {
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2"
  },
  "peerDependenciesMeta": {
    "express": {
      "optional": true
    },
    "jsonwebtoken": {
      "optional": false
    }
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.6",
    "typescript": "^5.3.3",
    "vitest": "^1.2.0"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ]
}
